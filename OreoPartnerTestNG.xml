<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">

    <listeners>
        <listener class-name="utility.Listeners"/>
        <!--<listener class-name="utility.ExtentReporterNG"/>-->
    </listeners>

    <parameter name="browserName" value="chrome"/>
    <parameter name="deviceName" value="Oreo Phone"/>
    <parameter name="udid" value="JF4L6SINCE8DMFU8"/>
    <parameter name="platformName" value="Android"/>
    <parameter name="appPackage" value="displayridepartner.displayridepartnersampleapp"/>
    <parameter name="appActivity" value="MainActivity"/>
    <parameter name="newCommandTimeout" value="3600"/>
    <parameter name="unicodeKeyboard" value="true"/>
    <parameter name="resetKeyboard" value="true"/>
    <parameter name="fullReset" value="false"/>
    <parameter name="noReset" value="true"/>
    <parameter name="portNumber" value="8000"/>
    <parameter name="riderName" value="Marshmallow"/>
    <parameter name="accessoryId" value=""/> <!--Add Accessory ID-->


    <test name="SendDemoToAccessory">
    <classes>
    <class name="tests.main_view_partner.SendDemoToAccessory"/>
    </classes>
    </test>

    <test name="UiElementsTextConsistency">
        <classes>
            <class name="tests.main_view_partner.UiElementsTextConsistencyTest"/>
        </classes>
    </test>


    <test name="SendImageToAccessoty">
        <classes>
            <class name="tests.main_view_partner.SendImageToAccessory"/>
        </classes>
    </test>


    <test name="SendVideoToAccessoty">
        <classes>
            <class name="tests.main_view_partner.SendVideoToAccessory"/>
        </classes>
    </test>


    <test name="SendTextToAccessoty">
        <classes>
            <class name="tests.main_view_partner.SendTextToAccessory"/>
        </classes>
    </test>


</suite>